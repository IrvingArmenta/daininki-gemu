{
  "name": "pokemon-x-y-landing",
  "description": "ポケモン　XとYのランディングページ",
  "version": "1.0.0",
  "private": true,
  "author": "Irving Armenta <irvingarmentajd@gmail.com> (https://github.com/IrvingArmenta)",
