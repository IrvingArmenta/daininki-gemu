    "styled-tools": "^1.7.2",
    "zustand": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^15.6.0",
    "@types/react": "17.0.6",
    "@types/react-dom": "17.0.5",
    "@types/styled-components": "^5.1.9",
